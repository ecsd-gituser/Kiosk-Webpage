{
  "name": "ECSD Kiosk APP",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "37.0.0.0",
  "permissions": [
    "webview",
    "power",
    "storage",
    "tabs",
    "activeTab"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "kiosk_enabled": true,
  "icons": {
    "128": "assets/icon.png"
  }
}
